#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int n;
    scanf("%d",&n);
    int a[n],i,s=0,r=0;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
         if(a[i]%2==0)
         {
                s=s+a[i];
          }
        else{
            
            r=r+a[i];
       }
    }
    printf("%d\n",r);
    printf("%d\n",s);
  return 0;
}
