#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{ int n;
 scanf("%d",&n);
 int a[n],i,l;
 for(i=0;i<n;i++)
 {
     scanf("%d",&a[i]);
 }
  l=a[0];
 for(i=1;i<n;i++)
 {
  if(l<a[i])
  {
              l=a[i];
  }
          
  }
 printf("%d",l);
  return 0;   
 }

