#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{ 
    char str[50];
    int c=0,i;
    scanf("%[^\n]s",str);
    for(i=0;str[i];i++)
    {
         if(str[i]=='a'|| str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||  
            str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U')
          {
            c++;
        }
    }  
        printf("%d",c);
    return 0;
}
