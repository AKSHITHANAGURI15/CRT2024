#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{ 
    char str[100];
    int i,s=0,dig=0;
    scanf("%s",str);
    for(i=0;str[i]!='\0';i++)
    {
        if(str[i]>=48&&str[i]<=57)
        {
             dig++;
            s=s+dig;
        }
    }
   printf("%d",s);
   
    return 0;
}
